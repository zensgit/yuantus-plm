# Local CADGF dev overrides (copy to .env.local)
# Usage:
#   cp .env.local.example .env.local
#   set -a; source .env.local; set +a
#   docker compose --env-file .env.local up -d api worker

# CADGF router (host)
CADGF_ROOT=/Users/huazhou/Downloads/Github/CADGameFusion-codex-yuantus
CADGF_PLUGIN_PATH=/Users/huazhou/Downloads/Github/CADGameFusion-legacy/build_vcpkg/plugins/libcadgf_json_importer_plugin.dylib
CADGF_CONVERT_CLI=/Users/huazhou/Downloads/Github/CADGameFusion-legacy/build_vcpkg/tools/convert_cli
CADGF_ROUTER_HOST=127.0.0.1
CADGF_ROUTER_PORT=9000

# Yuantus CADGF conversion (host worker)
YUANTUS_CADGF_ROOT=/Users/huazhou/Downloads/Github/CADGameFusion-codex-yuantus
YUANTUS_CADGF_CONVERT_CLI=/Users/huazhou/Downloads/Github/CADGameFusion-legacy/build_vcpkg/tools/convert_cli
YUANTUS_CADGF_DXF_PLUGIN_PATH=/Users/huazhou/Downloads/Github/CADGameFusion-legacy/build_vcpkg/plugins/libcadgf_json_importer_plugin.dylib
YUANTUS_CADGF_ROUTER_BASE_URL=http://127.0.0.1:9000
YUANTUS_CADGF_ROUTER_PUBLIC_BASE_URL=http://localhost:9000
YUANTUS_CADGF_DEFAULT_EMIT=json,gltf,meta

# Optional
# YUANTUS_CADGF_PYTHON_BIN=/opt/homebrew/bin/python3.12
# YUANTUS_DWG_CONVERTER_BIN=/path/to/dwg_converter
